												<div class="col-xs-12 col-sm-4 col-md-4 col-lg-3">{{userName}}</div>
												<div class="col-xs-12 col-sm-12 col-md-12 col-lg-3 symptoms">{{symptoms}}</div>
											</li>
										{{/memberPast.consultation}}
									</ul>
									{{#if memberPast.showViewMore}}
										<div class="pull-right userRoleSwitch"><a class="viewMore" href="javascript:void(0);" data-prop="memberPast">View More</a></div>
									{{/if}}
								</div>
							{{/if}}
						{{/if}}
					</div>
				{{else}}
					<div class="row">
						{{#if noConsultation}}
							<div class="noConsultation">

										{{/memberPast.consultation}}
									</ul>
									{{#if memberPast.showViewMore}}
										<div class="pull-right userRoleSwitch"><a class="viewMore" href="javascript:void(0);" data-prop="memberPast">View More</a></div>
									{{/if}}
								</div>
							{{/if}}
						{{/if}}
					</div>
				{{/if}}
			{{/if}}
			<div class="clear" style="clear:both;"></div>
		</div>
	</div>
</div>